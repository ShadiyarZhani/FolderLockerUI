      <ListView fx:id="fileListView" editable="true" layoutX="12.0" layoutY="67.0" prefHeight="136.0" prefWidth="495.0" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="12.0" AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="70.0" />
