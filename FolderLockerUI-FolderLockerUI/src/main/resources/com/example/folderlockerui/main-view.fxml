      <Button fx:id="EncryptBtn" layoutX="406.0" layoutY="210.0" mnemonicParsing="false" onAction="#EncryptDecrypt" text="Encrypt/Decrypt" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="12.0" />
